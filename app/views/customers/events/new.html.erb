<h1>イベントを投稿する</h1>
<%= form_with model: @event, url: customer_circle_events_path, local:true do |f| %>
  <h4>画像</h4>
  <%#= f.attachment_field :image %>
  <h4>イベント名</h4>
  <%= f.text_field :title %>
  <h4>イベント内容</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
  <h4>活動場所選択</h4>
  <%= f.collection_select :place, JpPrefecture::Prefecture.all, :code, :name, { prompt: '選択してください' }, class: 'form-control' %>
<% end %>